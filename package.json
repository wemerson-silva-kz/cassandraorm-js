{
  "name": "cassandraorm-js",
  "version": "1.0.0",
  "description": "The most advanced ORM for Apache Cassandra and ScyllaDB with native TypeScript support, AI/ML integration, and enterprise-grade features.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node --format esm",
    "test": "jest",
    "test:full": "bun test",
    "test:playground": "bun run ./playground/run-all-tests.ts",
    "test:quick": "bun run ./playground/test-quick.ts",
    "test:unique": "bun run ./playground/test-unique-simple.ts",
    "test:crud": "bun run ./playground/test-crud-simple.ts",
    "test:complete": "bun run ./playground/test-complete-orm.ts",
    "test:types": "bun run ./playground/test-final-all-types.ts",
    "test:ai": "bun run ./playground/11-ai-ml-features.test.ts",
    "test:events": "bun run ./playground/12-event-sourcing.test.ts",
    "test:transactions": "bun run ./playground/13-distributed-transactions.test.ts",
    "test:subscriptions": "bun run ./playground/14-real-time-subscriptions.test.ts",
    "test:graphql": "bun run ./playground/15-graphql-integration.test.ts",
    "test:performance": "jest test/performance --verbose",
    "test:integration": "bun run ./playground/17-complete-integration.test.ts",
    "test:docs": "ts-node test/documentation/run-all-tests.ts",
    "test:docs:session1": "jest test/documentation/session1-foundation --verbose",
    "test:docs:session2": "jest test/documentation/session2-data-queries --verbose",
    "test:docs:session3": "jest test/documentation/session3-middleware --verbose",
    "test:docs:session4": "jest test/documentation/session4-ai-realtime --verbose",
    "test:docs:session5": "jest test/documentation/session5-distributed --verbose",
    "test:docs:session6": "jest test/documentation/session6-integrations --verbose",
    "test:docs:examples": "jest test/documentation/session6-integrations/examples.test.ts --verbose",
    "test:ai-ml": "jest test/ai-ml --verbose",
    "test:distributed": "jest test/distributed --verbose",
    "test:ide-extensions": "jest test/ide-extensions --verbose",
    "test:enhanced": "jest test/core/enhanced-client.test.ts --verbose",
    "test:new-features": "jest test/ai-ml test/performance test/distributed test/core/enhanced-client.test.ts --verbose",
    "test:all-features": "jest test/ai-ml test/performance test/distributed test/ide-extensions test/core/enhanced-client.test.ts --verbose",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cassandra",
    "scylladb",
    "orm",
    "typescript",
    "database",
    "nosql",
    "ai",
    "ml",
    "vector-search",
    "event-sourcing",
    "cqrs",
    "graphql",
    "real-time",
    "distributed",
    "enterprise"
  ],
  "author": "Wemerson Silva",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wemerson-silva-kz/cassandraorm-js.git"
  },
  "bugs": {
    "url": "https://github.com/wemerson-silva-kz/cassandraorm-js/issues"
  },
  "homepage": "https://github.com/wemerson-silva-kz/cassandraorm-js#readme",
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "cassandra-driver": "^4.7.2",
    "openai": "^5.20.2",
    "redis": "^5.8.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^30.1.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.1",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "FEATURES.md",
    "examples/**/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  }
}
