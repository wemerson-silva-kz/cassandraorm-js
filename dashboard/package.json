{
  "name": "cassandraorm-dashboard",
  "version": "1.0.0",
  "description": "Web Dashboard for CassandraORM JS - Real-time monitoring and management",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc && npm run build:client",
    "build:client": "cd client && npm run build",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "ts-node src/server.ts",
    "dev:client": "cd client && npm run dev",
    "start": "node dist/server.js"
  },
  "keywords": ["cassandra", "orm", "dashboard", "monitoring"],
  "author": "CassandraORM Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.0",
    "socket.io": "^4.7.0",
    "cors": "^2.8.5",
    "cassandraorm-js": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0",
    "concurrently": "^8.0.0"
  }
}
